{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"isolatedModules": true,
		"moduleResolution": "node",
		"baseUrl": ".",
		"paths": {
			"@components/*": [
				"src/components/*"
			],
			"@data/*": [
				"src/data/*"
			],
			"@layouts/*": [
				"src/layouts/*"
			],
			"@utils/*": [
				"src/utils/*"
			]
		}
	}
}
